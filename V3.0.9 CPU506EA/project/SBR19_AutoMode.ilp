(*2*)
(* Network 0 *)
(*YCHIndico el modo en el que estoy trabajando0 -> EMERGENCIA 1 -> LOCAL2 -> REMOTO3 -> AUTOMATICO*)
LD    TRUE
MOVE    3, WorkingMode
(* Network 1 *)
(*YCHTomo el control de las salidas*)
LD    TRUE
S    %M102.3
(* Network 2 *)
(*YCHSelección de referencia0 -> REMOTO1 -> AI_02 -> AI_13 -> CNT_04 -> CNT_1*)
LD    TRUE
AND(
LD    TRUE
EQ    TypeRef, 0
MOVE    RemoteRef, LevelRef
OR(
LD    TRUE
EQ    TypeRef, 1
MOVE    AI0_mag, LevelRef
)
OR(
LD    TRUE
EQ    TypeRef, 2
MOVE    AI1_mag, LevelRef
)
OR(
LD    TRUE
EQ    TypeRef, 3
MOVE    FlowCNT0, LevelRef
)
OR(
LD    TRUE
EQ    TypeRef, 4
MOVE    FlowCNT1, LevelRef
)
)
(* Network 3 *)
(*YCHCONTROL POR NIVELES DE LLENADOApago cuando el nivel es mayor que el máximo y prendo cuando el nivel es menor que el mínimo*)
LD    TRUE
EQ    controlSystem, 0
AND(
LD    TRUE
GE    LevelRef, MaxLevel
R_BLK    1, ControlOut0
R_BLK    1, ControlOut1
OR(
LD    TRUE
LE    LevelRef, MinLevel
S_BLK    1, ControlOut0
S_BLK    1, ControlOut1
)
OR(
LD    TRUE
MOVE    mbFreqPump, FreqPump
)
OR(
LD    %M101.7
S_BLK    1, mbST0
)
OR(
LDN    %M101.7
R_BLK    1, mbST0
)
)
(* Network 4 *)
(*YCHCONTROL POR NIVELES DE VACIADOPrendo cuando el nivel es mayor que el máximo y apago cuando el nivel es menor que el mínimoLa señal de prendido y apagado de la bomba viene de la señal de ST0*)
LD    TRUE
EQ    controlSystem, 1
AND(
LD    TRUE
GE    LevelRef, MaxLevel
S_BLK    1, ControlOut0
S_BLK    1, ControlOut1
OR(
LD    TRUE
LE    LevelRef, MinLevel
R_BLK    1, ControlOut0
R_BLK    1, ControlOut1
)
OR(
LD    TRUE
MOVE    mbFreqPump, FreqPump
)
OR(
LD    %M101.7
S_BLK    1, mbST0
)
OR(
LDN    %M101.7
R_BLK    1, mbST0
)
)
(* Network 5 *)
(*YCHCONTROL EXTERNOLe sedo el control al sistema de emergencia o externo para que actue en este modoLa señal de prendido y apagado de la bomba viene de la señal de ST0*)
LD    TRUE
EQ    controlSystem, 2
AND(
LD    TRUE
MOVE    mbFreqPump, FreqPump
OR(
LD    %M101.7
S_BLK    1, mbST0
)
OR(
LDN    %M101.7
R_BLK    1, mbST0
)
)
R    %M102.3
(* Network 6 *)
(*YCHCONTROL POR CONSIGNA CONTINUASe intenta mantener la magnitud de referencia variando la salida analógica dedidaca a la frecuencia de un variadorDigo que la bomba está apagada cuando está la señal de frecuencia es menor a 5hz, en caso contrario está prendida si el ST0*)
LD    TRUE
EQ    controlSystem, 3
AND(
LD    TRUE
CAL    SBR30    DltRef, DltModOut, SloganSgn, LevelRef, FreqPump
S_BLK    1, ControlOut0
S_BLK    1, ControlOut1
OR(
LD    TRUE
LT    FreqPump, 10.0
R_BLK    1, mbST0
)
OR(
LD    %M101.7
GE    FreqPump, 10.0
S_BLK    1, mbST0
)
)
