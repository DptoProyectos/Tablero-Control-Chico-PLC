(*2*)
(* Network 0 *)
(*YCHIndico el modo en el que estoy trabajando0 -> EMERGENCIA 1 -> LOCAL2 -> REMOTO3 -> AUTOMATICO*)
LD    TRUE
MOVE    3, WorkingMode
(* Network 1 *)
(*YCHTomo el control de las salidas*)
LD    TRUE
S    %M102.3
(* Network 2 *)
(*YCHCONTROL POR NIVELES DE LLENADOApago cuando el nivel es mayor que el máximo y prendo cuando el nivel es menor que el mínimo*)
LD    TRUE
EQ    controlSystem, 0
AND(
LD    TRUE
GE    LevelRef, MaxLevel
R_BLK    1, ControlOut0
R_BLK    1, ControlOut1
OR(
LD    TRUE
LE    LevelRef, MinLevel
S_BLK    1, ControlOut0
S_BLK    1, ControlOut1
)
)
(* Network 3 *)
(*YCHCONTROL POR NIVELES DE VACIADOPrendo cuando el nivel es mayor que el máximo y apago cuando el nivel es menor que el mínimo*)
LD    TRUE
EQ    controlSystem, 1
AND(
LD    TRUE
GE    LevelRef, MaxLevel
S_BLK    1, ControlOut0
S_BLK    1, ControlOut1
OR(
LD    TRUE
LE    LevelRef, MinLevel
R_BLK    1, ControlOut0
R_BLK    1, ControlOut1
)
)
(* Network 4 *)
(*YCHCONTROL EXTERNOLe sedo el control al sistema de emergencia o externo para que actue en este modo*)
LD    TRUE
EQ    controlSystem, 2
R    %M102.3
(* Network 5 *)
(*YCHCONTROL POR CONSIGNAOJO -> FALTA POR PROGRAMAR*)
LD    TRUE
EQ    controlSystem, 3
